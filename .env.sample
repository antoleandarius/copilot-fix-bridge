# GitHub Configuration
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
GITHUB_REPO=your-username/your-repo-name

# JIRA Configuration
JIRA_BASE_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your_jira_api_token_here

# AgentHQ Configuration
AGENTHQ_API_KEY=agenthq_your_api_key_here
AGENTHQ_BASE_URL=https://api.agenthq.dev
AGENTHQ_AGENT_ID=agent_your_agent_id_here
AGENTHQ_WEBHOOK_URL=https://your-domain.com/agenthq-webhook
AGENTHQ_MOCK_MODE=true
AGENTHQ_TIMEOUT=30

# Feature Flags
ENABLE_AGENTHQ=false
FALLBACK_TO_GITHUB_ACTIONS=true

# Security
WEBHOOK_SECRET=change-this-to-a-random-secret

# Application Settings
PORT=8000
LOG_LEVEL=INFO
JSON_LOGS=false

# Retry Configuration
MAX_RETRIES=3
RETRY_BACKOFF_FACTOR=2.0
RETRY_INITIAL_DELAY=1.0
